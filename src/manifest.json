{
  "manifest_version": 3,
  "name": "webkansu",
  "version": "0.0.0.1",
  "description": "小説投稿サイトの機能を拡張します。",
  "content_scripts":[
    {
      "matches":[
        "http://ncode.syosetu.com/*"
      ],
      "js":[
        "./content_scripts/index.js"
      ]
    }
  ],
  "action": {
    "default_title": "webkansu のライブラリ画面を開きます",
    "default_popup": "./popup/index.html"
  }
}
